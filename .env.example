# Environment variables for AI FoundryIQ Demo
# Copy this file to .env and fill in your values

# Azure AI Foundry (Flinstones) - OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://flinstones.openai.azure.com/
AZURE_OPENAI_API_KEY=your-azure-openai-api-key-here
AZURE_OPENAI_API_VERSION=2024-08-01-preview
AZURE_OPENAI_DEPLOYMENT=gpt-4.1
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=text-embedding-ada-002

# Azure AI Search (stone-fabrix-agent-connector) Configuration
AZURE_SEARCH_ENDPOINT=https://stone-fabrix-agent-connector.search.windows.net
AZURE_SEARCH_API_KEY=your-azure-search-api-key-here
AZURE_SEARCH_INDEX_NAME=foundryiq-documents
